{
  "compileOnSave": false,
  "compilerOptions": {
    "skipLibCheck": true,
    "baseUrl": "",
    "rootDir": "./",
    "outDir": "./dist/out-tsc/src",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "esModuleInterop": true,
    "lib": ["esnext", "es2017", "dom"],
    "paths": {
      "@env/*": ["src/environments/*"],
      "@app/*": ["src/app/*"],
      "@xlayers/sketch-ingestor": ["dist/sketch-ingestor"],
      "@xlayers/sketch-ingestor/*": ["dist/sketch-ingestor/*"],
      "@xlayers/sketch-lib": ["dist/sketch-lib"],
      "@xlayers/sketch-lib/*": ["dist/sketch-lib/*"],
      "@xlayers/css-codegen": ["dist/css-codegen"],
      "@xlayers/css-codegen/*": ["dist/css-codegen/*"],
      "@xlayers/svg-codegen": ["dist/svg-codegen"],
      "@xlayers/svg-codegen/*": ["dist/svg-codegen/*"],
      "@xlayers/web-codegen": ["dist/web-codegen"],
      "@xlayers/web-codegen/*": ["dist/web-codegen/*"],
      "@xlayers/xaml-codegen": ["dist/xaml-codegen"],
      "@xlayers/xaml-codegen/*": ["dist/xaml-codegen/*"],
      "@xlayers/angular-codegen": ["dist/angular-codegen"],
      "@xlayers/angular-codegen/*": ["dist/angular-codegen/*"],
      "@xlayers/vue-codegen": ["dist/vue-codegen"],
      "@xlayers/vue-codegen/*": ["dist/vue-codegen/*"],
      "@xlayers/react-codegen": ["dist/react-codegen"],
      "@xlayers/react-codegen/*": ["dist/react-codegen/*"],
      "@xlayers/stencil-codegen": ["dist/stencil-codegen"],
      "@xlayers/stencil-codegen/*": ["dist/stencil-codegen/*"],
      "@xlayers/lit-element-codegen": ["dist/lit-element-codegen"],
      "@xlayers/lit-element-codegen/*": ["dist/lit-element-codegen/*"],
      "@xlayers/web-component-codegen": ["dist/web-component-codegen"],
      "@xlayers/web-component-codegen/*": ["dist/web-component-codegen/*"]
    }
  }
}
